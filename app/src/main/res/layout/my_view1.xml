<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.py.ysl.view.myview.MyView1
        android:id="@+id/my_view"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:paddingTop="40dp"
        android:paddingBottom="1dp"
        android:paddingLeft="150dp"
        android:paddingRight="2dp"
        custom:titleTextColor="#ff0000"
        custom:titleTextSize="40sp"
        android:visibility="gone"
        custom:titleText="4566"/>
<HorizontalScrollView
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.py.ysl.view.myview.BarGraphView
        android:id="@+id/bar_view"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        custom:bargrapColor="#22ffffff"
        custom:textSize="14sp"
        android:visibility="visible"
        custom:textColor="#79ABFF"
        />

</HorizontalScrollView>

    <com.py.ysl.view.myview.RoundView
        android:id="@+id/cicyle"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        custom:roundTextColor="#55000000"
        android:visibility="gone"
        custom:roundTextSize="12sp"/>
    <TextView
        android:id="@+id/text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="测试"/>


</LinearLayout>
